    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "postinstall": "ngcc",
        "build": "ng build",
        "test": "ng test --code-coverage=true --no-watch --no-progress --browsers ChromeHeadlessCI",
